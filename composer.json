{
    "name": "losysgim/seq-laravel-log",
    "description": "stormcode/seq-laravel-log sends log records to a Seq-Server.",
    "keywords": [
        "monolog",
        "handler",
        "seq",
        "structured logging"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikolaj Salamak",
            "email": "mikolaj.salamak@stormcode.pl"
        },
        {
            "name": "Markus Schlotbohm",
            "email": "markus.schlotbohm@gmx.de"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.2 || ^8.4",
        "ext-soap": "*",
        "monolog/monolog": "^3",
        "laravel/framework": "^11 || ^12",
        "php-http/client-implementation": "*",
        "php-http/guzzle7-adapter": "^1",
        "php-http/httplug": "^2",
        "php-http/message": "^1",
        "php-http/message-factory": "^1",
        "guzzlehttp/guzzle": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "StormCode\\SeqMonolog\\": "src"
        }
    }
}
